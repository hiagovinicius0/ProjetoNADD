<div id="cabecalho">
    <h5>Inserir Usuário</h5>
</div>
<div id="conteudo_create">
    <div class="row">
        <div class="col-md-12">
            <form method="post">
                <div class="form-group">
                    <label>Nome</label>
                    <input id="Nome" class="form-control" />
                </div>
                <div class="form-group">
                    <label>E-mail</label>
                    <input id="Email" class="form-control" />
                </div>
                <div class="form-group">
                    <label>Senha</label>
                    <input id="Senha" type="password" class="form-control" />
                </div>
            </form>
        </div>
    </div>
</div>
